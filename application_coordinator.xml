<coordinator-app name="${appName}_Coordinator" frequency="${coordinatorFrequency}" start="${startTime}" end="${endTime}" timezone="${timeZone}" xmlns="uri:oozie:coordinator:0.1">
   <controls>
      <timeout>${timeOut}</timeout>
   </controls>
   <action>
      <workflow>
         <app-path>${workflowPath}</app-path>
         <configuration>
            <property>
               <name>jobTracker</name>
               <value>${jobTracker}</value>
            </property>
            <property>
               <name>nameNode</name>
               <value>${nameNode}</value>
            </property>
            <property>
               <name>queueName</name>
               <value>${queueName}</value>
            </property>
            <property>
               <name>mappingString</name>
               <value>${mappingString}</value>
            </property>
            <property>
               <name>tableName</name>
               <value>${tableName}</value>
            </property>
            <property>
               <name>rootDir</name>
               <value>${rootDir}</value>
            </property>
            <property>
               <name>dbHost</name>
               <value>${dbHost}</value>
            </property>
            <property>
               <name>dbPort</name>
               <value>${dbPort}</value>
            </property>
            <property>
               <name>dbName</name>
               <value>${dbName}</value>
            </property>
            <property>
               <name>dbSchemaName</name>
               <value>${dbSchemaName}</value>
            </property>
            <property>
               <name>dbUser</name>
               <value>${dbUser}</value>
            </property>
            <property>
               <name>dbPasswordFile</name>
               <value>${dbPasswordFile}</value>
            </property>
            <property>
               <name>timeZone</name>
               <value>${timeZone}</value>
            </property>
            <property>
              <name>runDateTime</name>
              <value>${coord:formatTime(coord:dateOffset(coord:dateTzOffset(coord:nominalTime(), coord:conf("timeZone")),-1,'DAY'), "yyyyMMdd")}</value>
            </property>
         </configuration>
      </workflow>
   </action>
</coordinator-app>